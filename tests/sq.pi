n : 13
i : 1

sq : [n - i ( @s
                's'  : 0
                'c'  : 0 
                'sum': [i - 'c' (@s
                                   's' : 's' + i
                                   'c' : 'c' + 1
                                 )]
                i : i + 1
              )]
! : sq
